{
  "AppSettings": {
    "SiteTitle": "CommerceAuthoring_Sc9",
    "BootStrapFile": "Global",
    "DeploymentId": "Deployment01",
    "EnvironmentName": "HabitatAuthoring",
    "EncryptionProtectionType": "Machine",
    "EncryptionCertificateHash": "Enter a valid certificate thumbprint for a PXF file. X-509 not supported yet",
    "EncryptionSID": "Enter a valid SID for a domain registered user",
    "EncryptionKeyStorageLocation": "c:\\Encryption-Keys\\",
    "SitecoreIdentityServerUrl": "https://SX93identityserver.dev.local",
    "AllowedOrigins": [
      "https://localhost:4200",
      "https://bizfx.sc9.com",
      "https://sxa.storefront.com"
    ],
    "AntiForgeryEnabled": false,
    "CommerceServicesHostPostfix": "sc9.com",
    "UseHttpsInKestrel": true,
    "SslPort": 5000,
    "SslPfxPath": "wwwroot/localhost.pfx",
    "SslPfxPassword": "sitecore"
  },
    "ApplicationInsights":  {
                                "InstrumentationKey":  ""
                            },
    "Logging":  {
                    "IncludeScopes":  false,
      "LogLevel": {
        "Default": "Debug",
        "System": "Debug",
        "Microsoft": "Debug"
      },
                    "PipelineTraceLoggingEnabled":  true,
                    "SerilogLoggingEnabled":  true
                },
    "Serilog":  {
                    "Using":  [
                                  "Serilog.Sinks.Literate",
                                  "Serilog.Sinks.File",
                                  "Serilog.Sinks.ApplicationInsights"
                              ],
      "MinimumLevel": {
        "Default": "Debug",
        "Override": {
          "Microsoft": "Debug",
          "System": "Debug"
        }
      },
                    "WriteTo":  [
                                    {
                                        "Name":  "ApplicationInsights",
                                        "Args":  {
                                                     "restrictedToMinimumLevel":  "Warning",
                                                     "telemetryConverter":  "Serilog.Sinks.ApplicationInsights.Sinks.ApplicationInsights.TelemetryConverters.TraceTelemetryConverter, Serilog.Sinks.ApplicationInsights"
                                                 }
                                    }
                                ],
                    "FileSizeLimitBytes":  100000000
                },
    "Caching":  {
                    "Memory":  {
                                   "Enabled":  false,
                                   "CacheStoreName":  "Commerce-Memory-Store"
                               },
                    "Redis":  {
                                  "Enabled":  true,
                                  "CacheStoreName":  "Commerce-Redis-Store",
                                  "Options":  {
                                                  "Configuration":  "localhost",
                                                  "InstanceName":  "Redis"
                                              }
                              }
                },
    "Compression":  {
                        "Enabled":  true,
                        "ResponseCompressionOptions":  {
                                                           "EnableForHttps":  true,
                                                           "MimeTypes":  [
                                                                             "text/plain",
                                                                             "text/css",
                                                                             "application/javascript",
                                                                             "text/html",
                                                                             "application/xml",
                                                                             "text/xml",
                                                                             "application/json",
                                                                             "text/json",
                                                                             "application/wasm"
                                                                         ]
                                                       },
                        "GzipCompressionProviderOptions":  {
                                                               "Level":  "Fastest"
                                                           }
                    },
    "CommerceConnector":  {
                              "ClientId":  "CommerceEngineConnect"
                          }
}
